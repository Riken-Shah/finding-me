{
  "name": "personal-website-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "db:migrate": "wrangler d1 execute website-analytics-dev --local --file=./migrations/0001_analytics.sql",
    "db:migrate:prod": "wrangler d1 execute website-analytics --file=./migrations/0001_analytics.sql",
    "db:clean": "wrangler d1 delete website-analytics-dev --local && wrangler d1 create website-analytics-dev",
    "db:reset": "pnpm db:clean && pnpm db:migrate",
    "db:shell": "wrangler d1 execute website-analytics-dev --local --command '.tables'",
    "db:info": "wrangler d1 execute website-analytics-dev --local --command 'SELECT COUNT(*) as count FROM sessions; SELECT COUNT(*) as count FROM pageviews; SELECT COUNT(*) as count FROM events;'"
  },
  "dependencies": {
    "@tremor/react": "^3.18.6",
    "@types/leaflet": "^1.9.15",
    "cookie": "^1.0.2",
    "leaflet": "^1.9.4",
    "next": "15.1.0",
    "pigeon-maps": "^0.22.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "recharts": "2.15.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "^4.20241230.0",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "^39.2.5",
    "wrangler": "^3.99.0"
  }
}